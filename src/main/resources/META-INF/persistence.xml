	<persistence-unit name="sky" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>java:jboss/datasources/PostgreSQLDS</jta-data-source>

		<class>org.javaee7.emr.entities.GenId</class>
		<class>org.javaee7.emr.entities.GenSeq</class>
		<class>org.javaee7.emr.entities.LanguageResources</class>
		<class>org.javaee7.movieplex7.entities.Patient</class>


		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property name="eclipselink.target-server" value="JBoss" />
			<property name="eclipselink.target-database" value="PostgreSQL" />
			<property name="eclipselink.jdbc.uppercase-columns" value="false" />
			<property name="eclipselink.result-collection-type" value="java.util.ArrayList" />
			<property name="eclipselink.logging.level" value="FINE" />
			<property name="eclipselink.logging.level.sql" value="FINE" />
			<property name="eclipselink.logging.parameters" value="true" />
			<property name="jdbc.persistence.logging" value="true" />
		</properties>
	</persistence-unit>
</persistence>
